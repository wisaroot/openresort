	<queryString><![CDATA[select ga_accno as accno, gf_folno as folno,gt_folseq,gf_roomno as room,ga_arrival, ga_departure,gt_deptdesc,gt_trndate,gt_trntime,gt_amount, gt_depttype,( case when (ga_guestno > 0) then (t_id||gn_fname||' '||gn_lname) else gri_grpname end ),
             gf_adult, gf_child, a_name
           
        from gaccount ga
            left join gfolio gf on (ga_accno = gf_accno) left join gtran gt1 on (gt1.gt_folno=gf_folno)
            left join groupinfo grpi on (ga_grpno = gri_grpno)
            left join guestname gn
    on gn_guestno = ga_guestno left join title tt
    on t_id = gn_titleno
            left join agent ag on (ga_agentno = a_id)
            
        where (ga_accstat = 'I') and   gt_folseq = 2 and ( gt_folno = $P{fol} )
        order by gt_trnseq]]></queryString>

